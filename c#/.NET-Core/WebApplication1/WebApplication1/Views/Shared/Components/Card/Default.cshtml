@model List<WebApplication1.Models.User>
<div class="container">
    <div class="row">
        @foreach(var item in Model) {
            <div class="col-md-4">
                <div class="card">
                    <img src="..." class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">
                            @item.Name
                        </h5>
                        <p class="card-text">
                            @item.Surname
                        </p>
                        <p class="card-text"><small class="text-body-secondary">Last updated 3 mins ago</small></p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>