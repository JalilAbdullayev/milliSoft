@model WebApplication1.Models.User
<div class="card">
    <img src="..." class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">
            @Model.Name
        </h5>
        <p class="card-text">
            @Model.Surname
        </p>
        <p class="card-text"><small class="text-body-secondary">Last updated 3 mins ago</small></p>
    </div>
</div>